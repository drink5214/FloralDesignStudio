# Windsurf API Configuration
WINDSURF_API_KEY=your_api_key_here
WINDSURF_API_URL=https://api.windsurf.ai/v1

# Project Configuration
PROJECT_PATH=/path/to/your/xcode/project
XCODE_PROJECT_NAME=FloralDesignStudio
XCODE_SCHEME_NAME=FloralDesignStudio

# Build Configuration
BUILD_OUTPUT_PATH=/path/to/build/output
BUILD_CONFIGURATION=Debug

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=/path/to/logs/windsurf_integration.log

# Agent Configuration
FILE_MONITOR_DEBOUNCE_TIME=1.0  # seconds
BUILD_RETRY_LIMIT=3
ERROR_CORRECTION_TIMEOUT=300  # seconds